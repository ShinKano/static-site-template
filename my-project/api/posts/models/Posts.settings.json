{
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "posts",
    "description": "blog posts for react static blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "required": true,
      "type": "string"
    },
    "content": {
      "required": true,
      "type": "text"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}